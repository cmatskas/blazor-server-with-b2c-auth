{
  "AzureAdB2C": {
    "Instance": "https://<YourB2CName>.b2clogin.com/tfp/",
    "ClientId": "<Your Client Id>",
    "TenantId": "<Your Tenant Id>",
    "CallbackPath": "/signin-oidc",
    "Domain": "<YourB2CName>.onmicrosoft.com",
    "SignedOutCallbackPath": "/signout/<YourB2CPolicyName - all lower case>",
    "ClientSecret": "",
    "SignUpSignInPolicyId": "<YourB2CPolicyName - all lower case>"
  },
  "DownstreamApi": {
    "Scopes": "https://<YourB2CName>.onmicrosoft.com/<YourAPIGuid>/access_as_user"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
